        <allow>
            <principal>User4</principal>
            <permission>CONNECT</permission>
        </allow>
        <allow>
            <principal>User4</principal>
            <permission>CREATE</permission>
        </allow>
        <allow>
            <principal>User4</principal>
            <permission>READ</permission>
            <resource>Admin</resource>
